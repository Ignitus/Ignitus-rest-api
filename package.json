{
  "name": "ignitus-server",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": "13.2.0"
  },
  "scripts": {
    "build": "tsc",
    "postinstall": "npm run build",
    "start": "ts-node build/index.js",
    "start-dev": "nodemon",
    "tslint": "tslint -c tslint.json -p tsconfig.json"
  },
  "nyc": {
    "check-coverage": false,
    "all": true,
    "extension": [
      ".js"
    ],
    "include": [
      "**/*.js"
    ],
    "exclude": [
      "**/*.spec.js",
      "build",
      "node_modules"
    ],
    "collectCoverageFrom": [
      "./config/*.js"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "require": [
      "babel-register"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "dependencies": {
    "babel-register": "^6.26.0",
    "bcrypt": "^3.0.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "~1.18.2",
    "cookie-parser": "~1.4.3",
    "crypto": "^1.0.1",
    "express": "^4.16.3",
    "graphql-simple-cache": "0.0.13",
    "jsonwebtoken": "^8.2.0",
    "mongoose": "^5.0.12",
    "morgan": "~1.9.0",
    "node-linkedin": "^0.5.6",
    "nyc": "^15.0.0",
    "redis": "^2.8.0",
    "web-push": "^3.3.3",
    "typescript": "3.8.3",
    "ts-node": "8.7.0",
    "@types/cookie-parser": "1.4.2",
    "@types/express": "4.17.3",
    "@types/morgan": "1.9.0",
    "@types/node": "13.9.1",
    "@types/web-push": "3.3.0"
  },
  "devDependencies": {
    "eslint": "^5.6.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "nodemon": "1.19.4",
    "tslint": "6.1.0"
  }
}
