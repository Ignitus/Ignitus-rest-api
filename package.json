{
  "name": "loginregister",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "start-dev": "nodemon --watch app.js --watch bin --watch config --watch controllers --watch helper --watch middlewares --watch models --watch routes --watch views -e js,json ./bin/www",
    "lint": "eslint ./ --ext .js",
    "lint:fix": "eslint ./ --ext .js --fix",
    "test": "nyc mocha **/*.spec.js"
  },
  "nyc": {
    "check-coverage": false,
    "all": true,
    "extension": [
      ".js"
    ],
    "include": [
      "**/*.js"
    ],
    "exclude": [
      "**/*.spec.js",
      "build",
      "node_modules"
    ],
    "collectCoverageFrom": [
      "./config/*.js"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "require": [
      "babel-register"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "dependencies": {
    "babel-register": "^6.26.0",
    "bcrypt": "^3.0.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "~1.18.2",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.4",
    "crypto": "^1.0.1",
    "debug": "~2.6.9",
    "dotenv": "^6.1.0",
    "express": "^4.16.3",
    "jsonwebtoken": "^8.2.0",
    "mocha": "^5.2.0",
    "mongoose": "^5.0.12",
    "morgan": "~1.9.0",
    "multer": "^1.3.0",
    "node-linkedin": "^0.5.6",
    "nodemailer": "^4.6.5",
    "nyc": "^13.0.1",
    "pug": "2.0.0-beta11",
    "serve-favicon": "~2.4.5",
    "web-push": "^3.3.3"
  },
  "devDependencies": {
    "eslint": "^5.6.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "nodemon": "^1.18.3"
  }
}
